<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.moniterapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="Players"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gtaV_Int"
            android:label="GTA V"
            android:parentActivityName=".MainActivity"
            android:theme="@style/gtaV_theme" />

        <activity
            android:name=".vainglory"
            android:label="Vainglory"
            android:theme="@style/vg_theme"
            android:parentActivityName=".MainActivity" />

        <activity android:name=".vg_hero1"
            android:label="VG Hero"
            android:theme="@style/vg_theme"
            android:parentActivityName=".vainglory" />


        <activity
            android:name=".vg_hero"
            android:label="VG Hero"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>